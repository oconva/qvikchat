{
	"minify": true,
	"jsc": {
		"parser": {
			"syntax": "typescript",
			"tsx": false,
			"decorators": false,
			"dynamicImport": false
		},
		"target": "es2017",
		"loose": false,
		"transform": {
			"legacyDecorator": false,
			"decoratorVersion": "2022-03"
		},
		"keepClassNames": false,
		"baseUrl": "./src",
		"paths": {
			"/*": ["./*"]
		},
		"minify": {
			"compress": {
				"arguments": true,
				"ecma": "2017",
				"reduce_funcs": true
			},
			"mangle": {},
			"format": {
				"comments": false
			}
		}
	},
	"sourceMaps": true,
	"isModule": true,
	"module": {
		"type": "commonjs",
		"strictMode": true
	}
}
