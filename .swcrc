{
  "minify": false,
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": false,
      "dynamicImport": false
    },
    "target": "es2017",
    "loose": false,
    "keepClassNames": true,
    "baseUrl": "./src",
    "paths": {
      "/*": ["./*"]
    }
  },
  "sourceMaps": true,
  "isModule": true,
  "module": {
    "type": "commonjs",
    "strictMode": true,
    "noInterop": true
  }
}
